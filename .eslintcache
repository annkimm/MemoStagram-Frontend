t { email, password } = inputValue;\n\n    if (errorMesseage.length === 0) {\n      dispatch(loginLoading({ email, password }));\n    } else {\n      alert(EMAIL_FORMAT_TRY_AGAIN);\n    }\n  };\n  return (\n    <Wrap>\n      <div>\n        <Image>\n          <img src=\"/images/login_title.png\" alt=\"logo\" />\n        </Image>\n        <div>\n          <Input\n            className=\"login\"\n            type=\"text\"\n            placeholder=\"email\"\n            name=\"email\"\n            onChange={handleChange}\n            value={inputValue.email}\n            icon={['far', 'user']}\n          />\n          {errorMesseage && <Message>{errorMesseage}</Message>}\n          <Input\n            className=\"login\"\n            type=\"password\"\n            placeholder=\"password\"\n            name=\"password\"\n            onChange={handleChange}\n            value={inputValue.password}\n            icon={['far', 'keyboard']}\n          />\n        </div>\n        <div>\n          <LoginButton type=\"submit\" onClick={onClickLogin}>\n            login\n          </LoginButton>\n          <FindPwd href=\"/\">find password</FindPwd>\n        </div>\n      </div>\n    </Wrap>\n  );\n};\n\nexport default Login;\n","/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/store/reducers/userReducer.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/store/reducers/loginReducer.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/store/sagas/loginWatcher.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/utils/validate.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/pages/login.style.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/utils/consts.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/store/actions/loginStore.ts",[],"/Users/gimminji/study/project/instargram/MemoStagram-Frontend/src/sections/Header.tsx",["124"],